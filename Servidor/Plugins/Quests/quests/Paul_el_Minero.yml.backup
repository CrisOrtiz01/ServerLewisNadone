quests:
  aprendiz_minero:
    name: '&8Aprendiz de Minero'
    ask-message: '&8[Paúl]: &7¡Así que quieres ser minero! Empecemos con lo básico. La redstone te enseñará sobre la energía que necesitas canalizar, y el diamante será tu primera recompensa verdadera. Te enseñaré a extraer más rápido de lo que creías posible.'
    finish-message: '&8[Paúl]: &7¡Bien hecho, chico! Tus manos comenzarán a moverse más rápido cuando sostengan un pico.'
    requirements:
      level: 0
    rewards:
      commands:
      - effect give %player% haste 999999 0 true
      - tellraw %player% {"text":"Has obtenido Prisa Minera I de forma permanente","color":"dark_gray"}
      items:
      - TOKEN_MISION 5
      money: 100
      quest-points: 5
      permission: habilidades.prisaminera.1
    objectives:
      redstone_blocks:
        type: collect
        item: REDSTONE_BLOCK
        amount: 5
        data: []
      diamond:
        type: collect
        item: DIAMOND
        amount: 1
        data: []
      tokens:
        type: collect
        item: TOKEN_MISION
        amount: 40
        data:
          display-name: '&eToken de Misión'
    options:
      cooldown:
        enabled: true
        time: 86400
      repeat:
        enabled: true
        infinite: false
        amount: 1
      category: minero
      
